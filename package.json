{
  "name": "@geoql/v-maplibre",
  "version": "1.1.2",
  "description": "Vue 3 components for MapLibre GL - reactive map components with full TypeScript support",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./style.css": "./dist/v-maplibre.css",
    "./v-maplibre.css": "./dist/v-maplibre.css",
    "./dist/style.css": "./dist/v-maplibre.css",
    "./dist/v-maplibre.css": "./dist/v-maplibre.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --project tsconfig.build.json",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky || true",
    "release": "release-it",
    "prepublishOnly": "bun run lint && bun run format:check && bun run test:coverage && bun run build"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "dependencies": {
    "maplibre-gl": "^5.14.0",
    "pmtiles": "^4.3.0"
  },
  "devDependencies": {
    "@awmottaz/prettier-plugin-void-html": "^2.0.0",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@release-it/conventional-changelog": "^10.0.3",
    "@types/node": "^25.0.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "oxlint": "^1.32.0",
    "prettier": "^3.7.4",
    "release-it": "^19.1.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vitest": "^4.0.15",
    "vue": "^3.5.25",
    "vue-tsc": "^3.1.8"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vue",
    "vue3",
    "maplibre",
    "maplibre-gl",
    "map",
    "mapping",
    "gis",
    "geospatial",
    "components",
    "nuxt"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/geoql/v-maplibre.git"
  },
  "bugs": {
    "url": "https://github.com/geoql/v-maplibre/issues"
  },
